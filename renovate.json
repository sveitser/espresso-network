{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    ":dependencyDashboard"
  ],
  "packageRules": [
    {
      "matchManagers": ["cargo"],
      "managerFilePatterns": [
        "/(^|/)Cargo\\.toml$/",
        "/(^|/)Cargo\\.lock$/"
      ],
      "groupName": "Cargo lockfile updates"
    }
  ]
}
